@model Helpdesk.Api.Models.Usuario

<h2>Login</h2>

<form asp-action="Login" method="post">
    <label>Email:</label>
    <input type="email" name="Email" required />
    <br />

    <label>Senha:</label>
    <input type="password" name="Senha" required />
    <br />

    <button type="submit">Entrar</button>
</form>
