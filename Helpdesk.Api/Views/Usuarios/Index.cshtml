@model IEnumerable<Helpdesk.Api.Models.Usuario>

<h2>Usu√°rios Cadastrados</h2>

<table border="1" cellpadding="5">
    <tr>
        <th>Nome</th>
        <th>Email</th>
        <th>Tipo</th>
    </tr>
    @foreach (var u in Model)
    {
        <tr>
            <td>@u.Nome</td>
            <td>@u.Email</td>
            <td>@u.Tipo</td>
        </tr>
    }
</table>

<p><a href="/Usuarios/Logout">Sair</a></p>
